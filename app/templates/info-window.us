<% _(orders).each(function(order){ %>
  <div class="item">
    Info about work order:
    <div class="description"><%= order.description %></div>
    <div class="location">
    <%= order.address %>, <%= order.city %>, <%= order.state %>
    </div>
    <button class="add-to-route" data-id="<%= order.id %>">Add to route</button>
  </div>
<% }) %>
